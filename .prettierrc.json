{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "semi": false,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": true,
  "filepath": "src/App.vue",
  "parser": "vue",
  "plugins": [
    "prettier-eslint"
  ],
  "editorconfig": true,
  "prettier-eslint": {
    "eslintPath": "node_modules/eslint"
  }
}
